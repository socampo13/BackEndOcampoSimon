        <p>No se encontraron datos</p>
        <% } else { %>
    <table class="table table-striped table-dark">
        <thead>
            <tr>
                <th>Nombre producto</th>
                <th>Precio</th>
                <th>Img del producto</th>
            </tr>
        </thead>
        <tbody>
            <% for (const usuario of usuarios) { %>
                <tr>
                    <td><%= producto.nombre %></td>
                    <td><%= producto.precio %></td>
                    <td><%= producto.url %></td>
                </tr>
            <% } %>
        </tbody>
    </table>
    <% } %>
</section>